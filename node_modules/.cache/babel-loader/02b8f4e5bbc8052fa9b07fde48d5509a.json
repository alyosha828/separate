=\"$emit('resetModal', '')\"\r\n  >\r\n    <the-modal :modal-id=\"'exportTeamsModal'\">\r\n      <template #header>\r\n        <img src=\"@/assets/images/ic_teams-on.svg\" alt=\"teams\" />\r\n        <button\r\n          type=\"button\"\r\n          class=\"close\"\r\n          data-dismiss=\"modal\"\r\n          aria-label=\"Close\"\r\n          @click=\"$emit('resetModal', '')\"\r\n        >\r\n          <span aria-hidden=\"true\">&times;</span>\r\n        </button>\r\n      </template>\r\n\r\n      <p style=\"word-wrap: break-word;\">\r\n        {{ this.data.teams1 }}\r\n        <!-- 通常通りTeams会議に入室し「ウェブ上で参加 / ブラウ\r\n        <br />ザ上で続行」をクリックしてください。 -->\r\n      </p>\r\n      <VueSlickCarousel\r\n        :arrows=\"false\"\r\n        :speed=\"700\"\r\n        :autoplaySpeed=\"1500\"\r\n        :autoplay=\"true\"\r\n        :accessibility=\"false\"\r\n      >\r\n        <img src=\"@/assets/images/teams02-4x3.png\" alt=\"teamsImg\" />\r\n        <img src=\"@/assets/images/teams01-4x3.png\" alt=\"teamsImg\" />\r\n      </VueSlickCarousel>\r\n\r\n      <form>\r\n        <div class=\"form-group row\">\r\n          <label for=\"meetText\" class=\"col-sm-12 col-form-label text-center\"\r\n             style=\"word-wrap: break-word;\">\r\n            {{ this.data.teams2 }}\r\n            <!-- ミーティングURL -->\r\n            </label\r\n          >\r\n          <div class=\"col-sm-12\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"meetText\"\r\n              v-model=\"teamsId\"\r\n              placeholder=\"XXXXXX\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-check\">\r\n          <input\r\n            class=\"form-check-input\"\r\n            type=\"checkbox\"\r\n            name=\"agree\"\r\n            id=\"agree\"\r\n            v-model=\"isChecked\"\r\n          />\r\n          <label class=\"form-check-label\" for=\"agree\">\r\n            <!-- AI通訳の<a href=\"#\">利用規約</a>に同意する。 -->\r\n            {{ this.data.teams3 }}\r\n          </label>\r\n        </div>\r\n      </form>\r\n\r\n      <div style=\"display: flex;margin: auto;\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-primary\"\r\n          data-dismiss=\"modal\"\r\n          data-toggle=\"modal\"\r\n          data-target=\"#recording\"\r\n          @click=\"handle\"\r\n        >\r\n        \r\n          {{ this.data.teams4 }}\r\n          <!-- Teamsに入室して通訳を開始 -->\r\n        </button>\r\n      </div>\r\n    </the-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TheModal from \"./the-modal.vue\";\r\nimport VueSlickCarousel from \"vue-slick-carousel\";\r\nimport \"vue-slick-carousel/dist/vue-slick-carousel.css\";\r\n\r\nexport default {\r\n  components: {\r\n    TheModal,\r\n    VueSlickCarousel\r\n  },\r\n  props: {\r\n    data:{\r\n    },\r\n  },\r\n  data: () => ({\r\n    teamsId: \"\",\r\n    isChecked: false,\r\n  }),\r\n  methods: {\r\n    handle() {\r\n      if (!this.isChecked) return;\r\n      if (this.teamsId == \"\") return;\r\n\r\n      window.open(this.teamsId, \"_blank\");\r\n      this.$emit(\"resetModal\", \"\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/modals"}]}