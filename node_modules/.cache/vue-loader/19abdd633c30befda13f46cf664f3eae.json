yle=\"word-wrap: break-word;\">\r\n        {{ this.data.skype1 }}\r\n        <!-- Skypeに入室し、通訳を開始します。<br />\r\n        会議のURLを入力してください。 -->\r\n      </p>\r\n      <VueSlickCarousel\r\n        :arrows=\"false\"\r\n        :speed=\"700\"\r\n        :autoplaySpeed=\"1500\"\r\n        :autoplay=\"true\"\r\n        :accessibility=\"false\"\r\n      >\r\n        <img src=\"@/assets/images/skype-ex-01.png\" alt=\"skypeImg\" />\r\n        <img src=\"@/assets/images/skype-ex-02.png\" alt=\"skypeImg\" />\r\n      </VueSlickCarousel>\r\n\r\n      <form>\r\n        <div class=\"form-group row\">\r\n          <label for=\"skypeText\" class=\"col-sm-12 col-form-label text-center\"\r\n            >\r\n            {{ this.data.skype2 }}\r\n            <!-- ミーティングURL -->\r\n            </label\r\n          >\r\n          <div class=\"col-sm-12\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              id=\"skypeText\"\r\n              v-model=\"teamsId\"\r\n              placeholder=\"XXXXXX\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-check\">\r\n          <input\r\n            class=\"form-check-input\"\r\n            type=\"checkbox\"\r\n            name=\"agree\"\r\n            id=\"agree\"\r\n            v-model=\"isChecked\"\r\n          />\r\n          <label class=\"form-check-label\" for=\"agree\">\r\n            <!-- AI通訳の<a href=\"#\">利用規約</a>に同意する。 -->\r\n            {{ this.data.skype3 }}\r\n          </label>\r\n        </div>\r\n      </form>\r\n\r\n      <div style=\"display: flex;margin: auto;\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-primary\"\r\n          data-dismiss=\"modal\"\r\n          data-toggle=\"modal\"\r\n          data-target=\"#recording\"\r\n          @click=\"handle\"\r\n        >\r\n        {{ this.data.skype4 }}\r\n         <!-- Skypeに入室して通訳を開始 -->\r\n        </button>\r\n      </div>\r\n    </the-modal>\r\n  </div>\